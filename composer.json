{
  "name": "orangesix/php",
  "description": "Biblioteca de funções utilizadas no desenvolvimento de projetos PHP com Laravel.",
  "keywords": [
    "Orangesix",
    "Laravel"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Orangesix\\": "src",
      "Tests\\": "tests"
    },
    "files": [
      "src/Function/all/class.php",
      "src/Function/all/convert.php",
      "src/Function/date/hours.php",
      "src/Function/date/period.php",
      "src/Function/laravel/view.php"
    ]
  },
  "authors": [
    {
      "name": "Luiz Fernando Bernardes de Paula",
      "email": "nandovga@hotmail.com",
      "role": "developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.13",
    "pestphp/pest": "^4.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
